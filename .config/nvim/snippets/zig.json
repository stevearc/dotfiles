    "prefix": ["t.ees"],
    "body": ["try testing.expectEqualStrings($1);"]
  },
  "test": {
    "prefix": ["T"],
    "body": ["test \"$1\" {", "    $0", "}"]
  },
  "class": {
    "prefix": ["class"],
    "body": [
      "pub const $1 = struct {",
      "    const Self = @This();",
      "    $0",
      "}"
    ]
  },
  "unwrap": {
    "prefix": ["uw"],
    "body": ["if ($1) |$2| {", "    $0", "}"]
  }
