{
  "title": "stevearc's Chrome fixes",
  "rules": [
    {
      "description": "Chrome Ctrl/Cmd swap",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": ["left_control"]
            },
            "key_code": "t"
          },
          "conditions": [
            {
              "type": "frontmost_application_if",
              "description": "",
              "bundle_identifiers": ["com.google.Chrome"]
            }
          ],
          "to": [
            {
              "repeat": false,
              "key_code": "t",
              "modifiers": ["left_gui"]
            }
          ]
        },
        {
          "type": "basic",
